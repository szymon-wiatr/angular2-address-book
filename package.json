{
  "name": "app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && webpack --progress --profile",
    "ci": "npm run e2e && npm run test",
    "clean": "rimraf node_modules doc dist && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean && npm start",
    "predelayed-open": "npm run server",
    "delayed-open": "sleep 5 && opener http://localhost:9000",
    "docker-open-terminal": "docker exec -i -t ${PWD##*/} /bin/bash",
    "docker-server": "webpack-dev-server --host=0.0.0.0 --hot --inline  --progress --port 8080 & wait",
    "docker-start": "docker run --rm --name ${PWD##*/} -i -p 8080:8080 -v $(pwd)/package.json:/home/yeoman/client/package.json -v $(pwd)/src:/home/yeoman/client/src cmelion/generator-ng2-webpack:latest",
    "docker-stop": "docker stop ${PWD##*/}",
    "docs": "typedoc --options typedoc.json src/app/app.ts",
    "e2e": "protractor",
    "e2e-live": "protractor --elementExplorer",
    "lint": "tsconfig-lint",
    "new-component": "yo ng2-webpack:component",
    "new-directive": "yo ng2-webpack:directive",
    "new-interface": "yo ng2-webpack:interface",
    "new-pipe": "yo ng2-webpack:pipe",
    "new-service": "yo ng2-webpack:service",
    "postinstall": "npm run webdriver-update && typings install",
    "server": "webpack-dev-server --hot --inline  --progress --port 9000 & wait",
    "start": "npm run server",
    "test": "karma start",
    "test-watch": "karma start --no-single-run --auto-watch",
    "watch": "webpack --watch --progress --profile",
    "webdriver-start": "webdriver-manager start",
    "webdriver-update": "webdriver-manager update"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.16",
    "country-list": "0.0.3",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.35.0",
    "es7-reflect-metadata": "1.4.0",
    "lodash": "^4.0.1",
    "parse5": "1.4.2",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-notification-system": "^0.2.6",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "zone.js": "^0.6.10"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.2",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.0",
    "es6-promise-loader": "^1.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.4",
    "html-loader": "^0.4.0",
    "html-webpack-plugin": "^2.8.1",
    "istanbul-instrumenter-loader": "^0.1.3",
    "jasmine-core": "^2.3.4",
    "jasmine-spec-reporter": "^2.4.0",
    "json-loader": "^0.5.3",
    "karma": "0.13.19",
    "karma-chrome-launcher": "^0.2.1",
    "karma-coverage": "^0.5.2",
    "karma-jasmine": "^0.3.7",
    "karma-phantomjs-launcher": "1.0.0",
    "karma-phantomjs2-launcher": "^0.5.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-story-reporter": "^0.3.1",
    "karma-webpack": "1.7.0",
    "left-pad": "git://github.com/azer/left-pad.git#bff80e3ef0db0bfaba7698606c4f623433d14355",
    "node-sass": "^3.4.2",
    "null-loader": "git+https://github.com/webpack/null-loader.git",
    "open": "0.0.5",
    "opener": "^1.4.1",
    "postcss-loader": "^0.8.0",
    "protractor": "^3.1.1",
    "raw-loader": "0.5.1",
    "rimraf": "^2.5.1",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.8.1",
    "tsconfig-lint": "^0.5.0",
    "tslint": "^3.4.0",
    "tslint-loader": "^2.1.0",
    "typedoc": "^0.3.12",
    "typescript": "^1.8.0",
    "typings": "0.8.1",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1",
    "webpack-load-plugins": "^0.1.2"
  }
}
